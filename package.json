{
  "name": "sketch-sort-selected-layers",
  "description": "A handy plugin to sort layers or artboards by Y or by X position.",
  "version": "1.0.0",
  "engines": {
    "sketch": ">=49.0"
  },
  "skpm": {
    "name": "Sort Selected Layers",
    "manifest": "src/manifest.json",
    "main": "sketch-sort-selected-layers.sketchplugin",
    "assets": [
      "assets/**/*"
    ],
    "sketch-assets-file": "sketch-assets/icons.sketch"
  },
  "scripts": {
    "build": "skpm-build",
    "watch": "skpm-build --watch",
    "start": "skpm-build --watch --run",
    "postinstall": "npm run build && skpm-link"
  },
  "devDependencies": {
    "@skpm/builder": "^0.7.0"
  },
  "author": "Ludovic Loridan <ludovic.loridan@me.com>"
}
